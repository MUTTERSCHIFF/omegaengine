<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>OmegaGUI</id>
        <version>$version$</version>
        <authors>Bastian Eicher</authors>
        <licenseUrl>http://code.google.com/p/omegaengine/wiki/License</licenseUrl>
        <projectUrl>http://code.google.com/p/omegaengine/wiki/OmegaGUI</projectUrl>
        <iconUrl>http://omega.nanobyte.de/images/icon_omega.png</iconUrl>
        <description>OmegaGUI is a skinable GUI toolkit for the OmegaEngine with an XML file format und Lua scripting.
The AlphaEditor contains a WYSIWYG editor for the toolkit.</description>
        <summary>Skinable GUI toolkit for the OmegaEngine</summary>
        <dependencies>
            <dependency id="OmegaEngine.Backend" version="$version$" />
        </dependencies>
        <references>
            <reference file="OmegaGUI.dll" />
        </references>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
    </metadata>
    <files>
        <file src="..\build\Release\OmegaGUI.dll" target="lib\net20" />
        <file src="..\build\Release\OmegaGUI.xml" target="lib\net20" />

        <!-- Satellite assemblies (translations) -->
        <file src="..\build\Release\de\OmegaGUI.resources.dll" target="lib\net20\de" />

        <!-- Symbols and source code -->
        <file src="..\build\Release\OmegaGUI.pdb" target="lib\net20" />
        <file src="..\src\OmegaGUI\**\*.cs" target="src\OmegaGUI" />
    </files>
</package>